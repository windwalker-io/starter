{
    "name": "windwalker/starter",
    "description": "Windwalker Starter.",
    "homepage": "https://github.com/ventoviro/windwalker",
    "license": "GNU Lesser General Public License",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "windwalker/core" : "~2.0",
        "symfony/yaml" : "~2.0",
        "filp/whoops": "~1.0",
        "fzaninotto/faker": "~1.0",
        "monolog/monolog": "~1.0",

        "twig/twig" : "~1.0"
    },
    "require-dev": {
        "windwalker/phpunit-skeleton-generator": "2.*"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "Windwalker\\Composer\\StarterInstaller::rootInstall"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "2.1-dev"
        }
    }
}
